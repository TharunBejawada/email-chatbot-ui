#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tharunbejawada/Desktop/Nitish Work/email-chatbot-ui/node_modules/.pnpm/nuxi@3.28.0/node_modules/nuxi/bin/node_modules:/Users/tharunbejawada/Desktop/Nitish Work/email-chatbot-ui/node_modules/.pnpm/nuxi@3.28.0/node_modules/nuxi/node_modules:/Users/tharunbejawada/Desktop/Nitish Work/email-chatbot-ui/node_modules/.pnpm/nuxi@3.28.0/node_modules:/Users/tharunbejawada/Desktop/Nitish Work/email-chatbot-ui/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tharunbejawada/Desktop/Nitish Work/email-chatbot-ui/node_modules/.pnpm/nuxi@3.28.0/node_modules/nuxi/bin/node_modules:/Users/tharunbejawada/Desktop/Nitish Work/email-chatbot-ui/node_modules/.pnpm/nuxi@3.28.0/node_modules/nuxi/node_modules:/Users/tharunbejawada/Desktop/Nitish Work/email-chatbot-ui/node_modules/.pnpm/nuxi@3.28.0/node_modules:/Users/tharunbejawada/Desktop/Nitish Work/email-chatbot-ui/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nuxi/bin/nuxi.mjs" "$@"
else
  exec node  "$basedir/../nuxi/bin/nuxi.mjs" "$@"
fi
